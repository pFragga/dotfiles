# X preferences
xset b off &
xset r rate 300 50 &

# keybindings
sxhkd &

# music
[ ! -s "$XDG_CONFIG_HOME/mpd/pid" ] && mpd &

# notifications
dunst &

# hide cursor when inactive
xbanish -t 5 -m ne &

# screen color temprature
redshift &

# systray
stalonetray &

# networking
nm-applet &

# statusbar
slstatus &
