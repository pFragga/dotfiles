#!/bin/sh

[ -f ~/.config/X11/Xresources ] && xrdb -merge ~/.config/X11/Xresources &
[ -f ~/.local/share/wallpaper ] && xwallpaper --zoom ~/.local/share/wallpaper &
xset b off &
xset r rate 300 50 &

pidof -q mpd || mpd &
xbanish -t 5 -m ne &
dunst &
redshift &
xsidle slock setxkbmap us &
gnome-keyring-daemon &
statusbar &

# wm/de
exec dwm 2> /tmp/dwm.log
